{
  "name": "data_bureau",
  "version": "0.1.0",
  "homepage": "http://localhost/",
  "private": true,
  "dependencies": {
    "@dnd-kit/core": "6.0.8",
    "@reduxjs/toolkit": "1.9.3",
    "@testing-library/jest-dom": "5.16.1",
    "@testing-library/react": "12.1.2",
    "@testing-library/user-event": "13.5.0",
    "@types/jest": "29.4.0",
    "ag-grid-community": "30.0.2",
    "ag-grid-react": "30.0.2",
    "axios": "1.4.0",
    "classnames": "2.3.2",
    "customize-cra": "1.0.0",
    "formik": "2.2.9",
    "lodash": "4.17.21",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-joyride": "2.5.5",
    "react-redux": "8.0.5",
    "react-router-dom": "6.9.0",
    "react-scripts": "5.0.0",
    "react-use-websocket": "3.0.0",
    "redux-persist": "6.0.0",
    "redux-thunk": "2.4.2",
    "rfs": "10.0.0",
    "web-vitals": "2.1.3",
    "yup": "0.32.11",
    "yup-password": "0.2.2"
  },
  "engines": {
    "node": ">=19.3.0",
    "npm": ">=9.2.0"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "prettier": "prettier --write ./src"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "react-hooks/exhaustive-deps": "off",
      "no-unused-vars": "off",
      "@typescript-eslint/no-unused-vars": "error",
      "space-in-parens": [
        "error",
        "never"
      ],
      "no-multiple-empty-lines": "warn",
      "no-console": "warn",
      "no-use-before-define": "error",
      "indent": [
        "warn",
        2,
        {
          "SwitchCase": 1
        }
      ],
      "semi": [
        "warn",
        "never"
      ],
      "keyword-spacing": [
        "error",
        {
          "before": true,
          "after": true
        }
      ],
      "object-curly-spacing": [
        "error",
        "always"
      ],
      "comma-spacing": [
        "error",
        {
          "before": false,
          "after": true
        }
      ],
      "no-trailing-spaces": "error",
      "no-multi-str": "off",
      "import/no-duplicates": "error",
      "quotes": [
        "error",
        "single"
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/axios": "0.14.0",
    "@types/node": "18.14.6",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "autoprefixer": "10.4.2",
    "css-loader": "6.5.1",
    "postcss": "8.4.5",
    "postcss-loader": "6.2.1",
    "precss": "4.0.0",
    "prettier": "2.8.4",
    "react-app-rewired": "2.2.1",
    "style-loader": "3.3.1",
    "stylus": "0.56.0",
    "stylus-loader": "6.2.0",
    "typescript": "5.3.2"
  }
}
